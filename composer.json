{
    "name": "nswdpc/silverstripe-nsw-customerpaymentsplatform-fakegateway",
    "description": "A fake CPP gateway used for acceptance testing of the nswdpc/silverstripe-nsw-customerpaymentsplatform module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "nsw",
        "payments",
        "omnipay",
        "oauth2",
        "testing"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James",
            "homepage": "https://dpc.nsw.gov.au",
            "role": "Developer"
        }
    ],
    "require": {
        "nswdpc/silverstripe-nsw-customerpaymentsplatform" : "*",
        "firebase/php-jwt" : "^5",
        "kriswallsmith/buzz": "^1.2",
        "nyholm/psr7": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 | ^7 | ^8 | ^9",
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.18",
        "codeception/codeception" : "^2.5 | ^3 | ^4"
    },
    "config": {
        "sort-packages": true
    },
    "repositories" : [
        {
            "type": "vcs",
            "url": "git@github.com:nswdpc/silverstripe-nsw-customerpaymentsplatform.git"
        }
    ]
}
